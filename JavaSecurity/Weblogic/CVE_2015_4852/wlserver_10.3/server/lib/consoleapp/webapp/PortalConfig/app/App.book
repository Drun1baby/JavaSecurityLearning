<?xml version="1.0" encoding="UTF-8"?>
<portal:root xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0"
             xmlns:html="http://www.w3.org/1999/xhtml-netuix-modified/1.0.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:portal="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0"
             xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0 portal-support-1_0_0.xsd">
<!--
    Definition file for the deployment pages in the j2ee subsystem
    of the WebLogic Server administration console.

    This contains the following major page structures:

    AppApplicationBook - Pages for an ear
    AppDeploymentsBook - Domain-wide deployment table
    Application control and lifecycle assistants

    Copyright (c) 2004,2011, Oracle and/or its affiliates. All rights reserved.
-->
  <!-- Definition for the Application configuration pages -->
  <netuix:book definitionLabel="AppBook" title="book.title" markupName="book"
               presentationClass="invisible">
    <netuix:content>
      <netuix:book definitionLabel="AppApplicationBook" title="book.title" markupName="book"
                   backingFile="com.bea.console.utils.AppDeploymentBackingFile"
                   threadsafe="true"
                   presentationClass="wlsc-frame">
        <netuix:titlebar/>
        <netuix:content>
          <netuix:page markupName="page" definitionLabel="AppPages" title="not.used" hidden="true">
            <netuix:content>
              <netuix:layout type="no" markupName="NoLayout">
                <netuix:placeholder usingFlow="false" markupName="ph1">
                  <netuix:book markupName="book" definitionLabel="AppTabsBook" title="AppTabsBook">
                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                    <netuix:content>
                      <!-- Definition for the Overview tab -->
                      <netuix:page markupName="page" definitionLabel="AppApplicationOverviewPage"
                                   presentationClass="page-content"
                                   title="tab.overview.label">
                        <netuix:meta name="helpid" content="J2EE.app.applicationoverview.title;j2ee"/>
                        <netuix:meta name="breadcrumb-context" content="handle"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationOverviewPortlet"
                                                      contentUri="/PortalConfig/app/ApplicationOverview.portlet"/>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the Deployment Plan tab -->
                      <netuix:book markupName="book" definitionLabel="AppApplicationDeploymentPlanPage"
                                   title="tab.deploymentplan.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Tuning Parameters tab -->
                          <netuix:page markupName="page" definitionLabel="AppApplicationTuningParameters"
                                       presentationClass="page-content"
                                       title="tab.tunables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationtunables.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Tuning Parameter pages -->
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationTunablesHeaderPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationTunablesHeader.portlet"/>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="AppApplicationTuningParametersBook">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="AppApplicationTunablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Tuning Parameter pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationTunablesTreePortlet"
                                                                      contentUri="/PortalConfig/app/ApplicationTunablesTree.portlet"/>
                                            </netuix:placeholder>
                                            <!-- Content pane for Tuning Parameter pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="AppApplicationTunablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab configuration page -->
                                                  <netuix:book markupName="book" definitionLabel="ApplicationTunablesContentsDefaultPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="ApplicationTunablesContentsDefaultPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="ApplicationTunablesContentsDefaultPortlet"
                                                                                      contentUri="/PortalConfig/app/ApplicationTunablesContentsDefault.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The single tunable tab for an Ear -->
                                                  <netuix:book markupName="book" definitionLabel="ApplicationTunablesApplicationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="ApplicationTunablesApplicationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.application.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portlet definitionLabel="ApplicationTunablesApplicationPortlet">
                                                                <netuix:strutsContent module="/j2ee" action="ApplicationTunablesApplication"
                                                                                      refreshAction="ApplicationTunablesApplication"/>
                                                              </netuix:portlet>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The two tunable tabs for a WebApp module -->
                                                  <netuix:book markupName="book" definitionLabel="ApplicationTunablesWebAppPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="WebAppTunablesGeneralPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.general.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppTunablesGeneralPortlet"
                                                                                      contentUri="/PortalConfig/webapp/WebAppTunablesGeneral.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                      <netuix:page markupName="page" definitionLabel="WebAppTunablesLoggingPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.logging.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppTunablesLoggingPortlet"
                                                                                      contentUri="/PortalConfig/webapp/WebAppTunablesLogging.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The single tunable tab for an EJB -->
                                                  <netuix:book markupName="book" definitionLabel="AppEJBTunablesPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="AppEJBTunablesPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.config.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="AppEJBTunablesPortlet"
                                                                                      contentUri="/PortalConfig/ejb/EJBTunables.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The single tunable tab for a web service -->
                                                  <netuix:book markupName="book" definitionLabel="WebServiceTunablesPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="WebServiceTunablesGeneralPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.general.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebServiceTunablesGeneralPortlet"
                                                                                      contentUri="/PortalConfig/webservice/WebServiceTunablesGeneral.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book" definitionLabel="ApplicationTunablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="ApplicationTunablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="ApplicationTunablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Resource Dependencies tab -->
                          <netuix:page markupName="page" definitionLabel="AppApplicationBindablesPages"
                                       presentationClass="page-content"
                                       title="tab.bindables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationdescriptors.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Dependencies pages -->
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationBindablesPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationBindablesHeader.portlet"/>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="AppApplicationBindablesContentsBooks">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="AppApplicationBindablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Dependencies pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationBindablesTreePortlet"
                                                                      contentUri="/PortalConfig/app/ApplicationBindablesTree.portlet"/>
                                            </netuix:placeholder>
                                            <!-- Content pane for Dependencies pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="AppApplicationBindablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab resource configuration page -->
                                                  <netuix:book markupName="book" definitionLabel="AppApplicationBindablesContentsPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="AppApplicationBindablesContentsPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationBindablesContentsPortlet"
                                                                                      contentUri="/PortalConfig/app/ApplicationBindablesContents.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book" definitionLabel="ApplicationBindablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="ApplicationBindablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="ApplicationBindablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the Configuration tab -->
                      <netuix:book markupName="book" definitionLabel="AppApplicationConfigurationPage"
                                   title="tab.configuration.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Application tab -->
                          <netuix:page markupName="page" definitionLabel="ApplicationConfigurationApplicationPage"
                                       presentationClass="page-content"
                                       title="tab.application.label">
                            <netuix:meta name="helpid" content="J2EE.app.appconfiguration.title;j2ee"/>
                            <netuix:meta name="breadcrumb-context" content="handle"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="ApplicationConfigurationApplicationPortlet">
                                    <netuix:strutsContent module="/j2ee" action="ApplicationConfigurationApplication"
                                                          refreshAction="ApplicationConfigurationApplication"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Persistence tab -->
                          <netuix:page markupName="page" definitionLabel="AppApplicationPersistenceUnitTablePage"
                                       presentationClass="page-content"
                                       title="tab.persistenceunit.table.label">
                            <netuix:meta name="helpid" content="J2EE.persistenceunit.table.title;j2ee"/>
                            <netuix:meta name="breadcrumb-context" content="handle"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationPersistenceUnitTablePortlet"
                                                          contentUri="/PortalConfig/persistence/PersistenceUnitTable.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Workload tab -->
                          <netuix:page markupName="page" definitionLabel="AppApplicationConfigurationWorkloadPage"
                                       presentationClass="page-content"
                                       title="tab.workload.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationconfigurationworkload.title;j2ee"/>
                            <netuix:meta name="breadcrumb-context" content="handle"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="AppApplicationConfigurationWorkloadPortlet">
                                    <netuix:strutsContent module="/j2ee" action="AppApplicationConfigurationWorkload"
                                                          refreshAction="AppApplicationConfigurationWorkload"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for the Instrumentation subtab-->
                          <netuix:page markupName="page"
                                       definitionLabel="AppApplicationConfigurationInstrumentationPage"
                                       presentationClass="page-content"
                                       title="tab.diagnostics.instrumentation.label">
                            <netuix:meta name="helpid"
                                         content="Diagnostics.diagnostics.moduleconfiginstrumentation.title;diagnostics"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="AppApplicationConfigurationInstrumentationPortlet"
                                                          contentUri="/PortalConfig/diagnostics/module/ModuleConfigInstrumentation.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the security tab -->
                      <netuix:book markupName="book" definitionLabel="AppApplicationSecurityPage"
                                   title="tab.security.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- definition for app roles table -->
                          <netuix:page markupName="page" definitionLabel="AppRolesTablePage"
                                       presentationClass="page-content"
                                       title="tab.roles.label">
                            <netuix:meta name="helpid" content="J2EE.app.approle.table.title;j2ee"/>
                            <netuix:meta name="breadcrumb" content="tab.roles.label;key"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="AppApplicationRolesTablePortlet">
                                    <netuix:strutsContent module="/j2ee" action="AppApplicationRolesTable"
                                                          refreshAction="AppApplicationRolesTable"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for app policies page -->
                          <netuix:page markupName="page" definitionLabel="AppApplicationSecurityPoliciesPage"
                                       presentationClass="page-content"
                                       title="tab.policies.label">
                            <netuix:meta name="helpid" content="J2EE.app.apppolicies.title;j2ee"/>
                            <netuix:meta name="breadcrumb" content="tab.policies.label;key"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="AppApplicationSecurityPoliciesPortlet">
                                    <netuix:pageflowContent contentUri="/com/bea/console/actions/app/apppolicies/EditAppPolicies.jpf"
                                                            action="begin"
                                                            refreshAction="begin"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the targets tab -->
                      <netuix:page markupName="page" definitionLabel="AppApplicationTargetsPage"
                                   presentationClass="page-content"
                                   title="tab.targets.label">
                        <netuix:meta name="helpid" content="J2EE.app.applicationtargets.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="AppApplicationTargetsPortlet">
                                <netuix:strutsContent module="/j2ee" action="AppApplicationTargetTable"
                                                      refreshAction="AppApplicationTargetTable"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the control tab -->
                      <netuix:book markupName="book" definitionLabel="AppControlPages" title="tab.control.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Lifecycle tab -->
                          <netuix:page markupName="page" definitionLabel="AppApplicationControlPage"
                                       presentationClass="page-content"
                                       title="tab.startstop.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationcontrol.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="AppApplicationControlPortlet">
                                    <netuix:strutsContent module="/j2ee" action="AppApplicationControl"
                                                          refreshAction="AppApplicationControl"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Initialize Caches tab -->
                          <netuix:page markupName="page" definitionLabel="AppControlCachesPage"
                                       presentationClass="page-content"
                                       title="tab.caches.label">
                            <netuix:meta name="helpid" content="J2EE.app.appcontrolcaches.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="AppControlCachesPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationControlCaches.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the testing tab -->
                      <netuix:page markupName="page" definitionLabel="AppApplicationTestingPage"
                                   presentationClass="page-content"
                                   title="tab.testing.label">
                        <netuix:meta name="helpid" content="J2EE.app.applicationtesting.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationTestingPortlet"
                                                      contentUri="/PortalConfig/app/ApplicationTesting.portlet"/>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
