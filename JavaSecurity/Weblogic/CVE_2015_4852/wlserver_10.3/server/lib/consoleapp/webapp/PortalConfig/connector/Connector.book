<?xml version="1.0" encoding="UTF-8"?>
<portal:root xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0"
             xmlns:html="http://www.w3.org/1999/xhtml-netuix-modified/1.0.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:portal="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0"
             xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0 portal-support-1_0_0.xsd">
<!--
    Definition file for the Connector pages in the j2ee subsystem
    of the WebLogic Server administration console.

    There are several main branches in this book:

     - ConnectorApplicationBook is used for a standalone Connector
     - ConnectorModuleBook is used for a Connector module within another app

    Copyright (c) 2004,2011, Oracle and/or its affiliates. All rights reserved.
-->
  <netuix:book definitionLabel="ConnectorBook" title="book.title" markupName="book"
               presentationClass="invisible">
    <netuix:content>
      <!-- Definition for the ConnectorApplicationBook configuration pages -->
      <netuix:book definitionLabel="ConnectorApplicationBook" title="book.title" markupName="book"
                   backingFile="com.bea.console.utils.AppDeploymentBackingFile"
                   threadsafe="true"
                   presentationClass="wlsc-frame">
        <netuix:titlebar/>
        <netuix:content>
          <netuix:page markupName="page" definitionLabel="ConnectorApplicationPages" title="not.used"
                       hidden="true">
            <netuix:content>
              <netuix:layout type="no" markupName="NoLayout">
                <netuix:placeholder usingFlow="false" markupName="ph1">
                  <netuix:book markupName="book" definitionLabel="ConnectorApplicationTabsBook"
                               title="ConnectorApplicationTabsBook">
                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                    <netuix:content>
                      <!-- Definition for the Overview tab -->
                      <netuix:page markupName="page" definitionLabel="ConnectorApplicationOverviewPage"
                                   presentationClass="page-content"
                                   title="tab.overview.label">
                        <netuix:meta name="helpid" content="J2EE.connector.connectoroverview.title;j2ee"/>
                        <netuix:meta name="breadcrumb-context" content="handle"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="ConnectorApplicationOverviewPortlet">
                                <netuix:strutsContent module="/j2ee" action="ConnectorApplicationOverview"
                                                      refreshAction="ConnectorApplicationOverview"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the Deployment Plan tab -->
                      <netuix:book markupName="book" definitionLabel="ConnectorApplicationDeploymentPlanPage"
                                   title="tab.deploymentplan.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Tuning Parameters tab -->
                          <netuix:page markupName="page" definitionLabel="ConnectorApplicationTuningParameters"
                                       presentationClass="page-content"
                                       title="tab.tunables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationtunables.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Tuning Parameter pages -->
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="ConnectorApplicationTunablesHeaderPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationTunablesHeader.portlet"/>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="ConnectorApplicationTuningParametersBook">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="ConnectorApplicationTunablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Tuning Parameter pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portletInstance markupType="Portlet" instanceLabel="ConnectorApplicationTunablesTreePortlet"
                                                                      contentUri="/PortalConfig/app/ApplicationTunablesTree.portlet"/>
                                            </netuix:placeholder>
                                            <!-- Content pane for Tuning Parameter pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="ConnectorApplicationTunablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab configuration page -->
                                                  <netuix:book markupName="book"
                                                               definitionLabel="ConnectorApplicationTunablesContentsDefaultPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page"
                                                                   definitionLabel="ConnectorApplicationTunablesContentsDefaultPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="ConnectorApplicationTunablesContentsDefaultPortlet"
                                                                                      contentUri="/PortalConfig/app/ApplicationTunablesContentsDefault.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book"
                                                               definitionLabel="ConnectorApplicationTunablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page"
                                                                   definitionLabel="ConnectorApplicationTunablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="ConnectorApplicationTunablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Resource Dependencies tab -->
                          <netuix:page markupName="page" definitionLabel="ConnectorApplicationBindablesPages"
                                       presentationClass="page-content"
                                       title="tab.bindables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationdescriptors.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Dependencies pages -->
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="ConnectorApplicationBindablesPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationBindablesHeader.portlet"/>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="ConnectorApplicationBindablesContentsBooks">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="ConnectorApplicationBindablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Dependencies pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portletInstance markupType="Portlet" instanceLabel="ConnectorApplicationBindablesTreePortlet"
                                                                      contentUri="/PortalConfig/app/ApplicationBindablesTree.portlet"/>
                                            </netuix:placeholder>
                                            <!-- Content pane for Dependencies pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="ConnectorApplicationBindablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab resource configuration page -->
                                                  <netuix:book markupName="book" definitionLabel="ConnectorApplicationBindablesContentsPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="ConnectorApplicationBindablesContentsPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="ConnectorApplicationBindablesContentsPortlet"
                                                                                      contentUri="/PortalConfig/app/ApplicationBindablesContents.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book"
                                                               definitionLabel="ConnectorApplicationBindablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page"
                                                                   definitionLabel="ConnectorApplicationBindablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="ConnectorApplicationBindablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the Configuration tab -->
                      <netuix:book markupName="book" definitionLabel="ConnectorApplicationConfigurationPage"
                                   title="tab.config.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for Resource Adapter General page -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationConfigurationGeneralPage"
                                       presentationClass="page-content"
                                       title="tab.general.label">
                            <netuix:meta name="helpid" content="J2EE.connector.configurationgeneral.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="ConnectorApplicationConfigurationGeneralPortlet">
                                    <netuix:strutsContent module="/connector" action="ConnectorConfigurationGeneral"
                                                          refreshAction="ConnectorConfigurationGeneral"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for Resource Adapter General Properties page -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationConfigurationPropertiesPage"
                                       presentationClass="page-content"
                                       title="tab.properties.label">
                            <netuix:meta name="helpid" content="J2EE.connector.configurationproperties.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationConfigurationPropertiesPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorConfigurationProperties.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for Outbound Connection Pools page -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationConfigurationOutboundConnectionPoolPage"
                                       presentationClass="page-content"
                                       title="tab.outboundconnectionpools.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.configurationoutboundconnections.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationConfigurationOutboundConnectionPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorConfigurationOutboundConnection.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for Admin Objects page -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationConfigurationAdminObjectsPage"
                                       presentationClass="page-content"
                                       title="tab.adminobjects.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.connectorconfigurationadminobjects.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationConfigurationAdminObjectsPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorConfigurationAdminObjects.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for Workload page -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationConfigurationWorkloadPage"
                                       presentationClass="page-content"
                                       title="tab.workload.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.connectorconfigurationworkload.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="ConnectorApplicationConfigurationWorkloadPortlet">
                                    <netuix:strutsContent module="/connector" action="ConnectorApplicationConfigurationWorkload"
                                                          refreshAction="ConnectorApplicationConfigurationWorkload"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for the Instrumentation subtab-->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationConfigurationInstrumentationPage"
                                       presentationClass="page-content"
                                       title="tab.diagnostics.instrumentation.label">
                            <netuix:meta name="helpid"
                                         content="Diagnostics.diagnostics.moduleconfiginstrumentation.title;diagnostics"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationConfigurationInstrumentationPortlet"
                                                          contentUri="/PortalConfig/diagnostics/module/ModuleConfigInstrumentation.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the security tab -->
                      <netuix:book markupName="book" definitionLabel="ConnectorApplicationSecurityPage"
                                   title="tab.security.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- definition for connector application roles table page -->
                          <netuix:page markupName="page" definitionLabel="ConnectorApplicationSecurityRolesTablePage"
                                       presentationClass="page-content"
                                       title="tab.roles.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.connectorapplicationrole.table.title;j2ee"/>
                            <netuix:meta name="breadcrumb" content="tab.roles.label;key"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="ConnectorApplicationSecurityRolesTablePortlet">
                                    <netuix:strutsContent module="/j2ee" action="ConnectorApplicationSecurityRolesTable"
                                                          refreshAction="ConnectorApplicationSecurityRolesTable"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for connector app policies page -->
                          <netuix:page markupName="page" definitionLabel="ConnectorApplicationSecurityPoliciesPage"
                                       presentationClass="page-content"
                                       title="tab.policies.label">
                            <netuix:meta name="helpid" content="J2EE.connector.application.policies.page.title;j2ee"/>
                            <netuix:meta name="breadcrumb" content="tab.policies.label;key"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="ConnectorApplicationSecurityPoliciesPortlet">
                                    <netuix:pageflowContent contentUri="/com/bea/console/actions/connector/policies/edit/app/EditConnectorAppPolicies.jpf"
                                                            action="begin"
                                                            refreshAction="begin"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for credential map page -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationSecurityCredentialMapPage"
                                       presentationClass="page-content"
                                       title="tab.credentialmapping.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.securitycredentialmappingtable.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationSecurityCredentialMapPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorSecurityCredentialMap.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for principals page -->
                          <netuix:page markupName="page" definitionLabel="ConnectorApplicationSecurityPrincipalPage"
                                       presentationClass="page-content"
                                       title="tab.principals.label">
                            <netuix:meta name="helpid" content="J2EE.connector.connectorsecurityprincipals.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationSecurityPrincipalsPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorSecurityPrincipals.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the targets tab -->
                      <netuix:page markupName="page" definitionLabel="ConnectorApplicationTargetsPage"
                                   presentationClass="page-content"
                                   title="tab.targets.label">
                        <netuix:meta name="helpid" content="J2EE.connector.connectortargets.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="ConnectorApplicationTargetsPortlet">
                                <netuix:strutsContent module="/j2ee" action="ConnectorTargets" refreshAction="ConnectorTargets"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>

                      <!-- Definition for the control tab -->
                      <netuix:book markupName="book" definitionLabel="ConnectorApplicationControlBook"
                                   title="tab.control.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Resource Adapter tab -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationControlResourceAdapterPage"
                                       presentationClass="page-content"
                                       title="tab.resourceadapter.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.connectorapplicationcontrolresourceadapter.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="ConnectorApplicationControlResourceAdapterPortlet">
                                    <netuix:strutsContent module="/j2ee" action="ConnectorApplicationControlResourceAdapter"
                                                          refreshAction="ConnectorApplicationControlResourceAdapter"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Outbound Connection Pools tab -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationControlConnectionPoolsPage"
                                       presentationClass="page-content"
                                       title="tab.outboundconnectionpools.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.connectorcontrolconnectionpools.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationControlConnectionPoolsPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorControlConnectionPools.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>

                      <!-- Definition for the testing tab -->
                      <netuix:page markupName="page" definitionLabel="ConnectorApplicationTestingPage"
                                   presentationClass="page-content"
                                   title="tab.testing.label">
                        <netuix:meta name="helpid" content="J2EE.connector.connectortesting.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portletInstance markupType="Portlet" instanceLabel="ConnectorApplicationTestingPortlet"
                                                      contentUri="/PortalConfig/connector/ConnectorTesting.portlet"/>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the monitoring tab -->
                      <netuix:book markupName="book" definitionLabel="ConnectorApplicationMonitoringBook"
                                   title="tab.monitoring.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Inbound Listeners tab -->
                          <netuix:page markupName="page"
                                       definitionLabel="ConnectorApplicationMonitorInboundListenersPage"
                                       presentationClass="page-content"
                                       title="tab.inboundlisteners.label">
                            <netuix:meta name="helpid"
                                         content="J2EE.connector.connectormonitorinboundlisteners.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="ConnectorApplicationMonitorInboundListenersPortlet"
                                                          contentUri="/PortalConfig/connector/ConnectorMonitorInboundListeners.portlet"/>
                                </netuix:placeholder>
