<?xml version="1.0" encoding="UTF-8"?>
<portal:root xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0"
             xmlns:html="http://www.w3.org/1999/xhtml-netuix-modified/1.0.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:portal="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0"
             xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0 portal-support-1_0_0.xsd">
<!--
    Definition file for the WebApp pages in the j2ee subsystem
    of the WebLogic Server administration console.

    There are several main branches in this book:

     - WebAppApplicationBook is used for a standalone WebApp
     - WebAppModuleBook is used for a WebApp module within another app

    Copyright (c) 2004,2011, Oracle and/or its affiliates. All rights reserved.
-->
  <!-- Definition for the WebAppApplicationBook configuration pages -->
  <netuix:book definitionLabel="WebAppApplication" title="book.title" markupName="book"
               presentationClass="invisible">
    <netuix:content>
      <netuix:book definitionLabel="WebAppApplicationBook" title="book.title" markupName="book"
                   backingFile="com.bea.console.utils.AppDeploymentBackingFile"
                   threadsafe="true"
                   presentationClass="wlsc-frame">
        <netuix:titlebar/>
        <netuix:content>
          <netuix:page markupName="page" definitionLabel="WebAppApplicationPages" title="not.used"
                       hidden="true">
            <netuix:content>
              <netuix:layout type="no" markupName="NoLayout">
                <netuix:placeholder usingFlow="false" markupName="ph1">
                  <netuix:book markupName="book" definitionLabel="WebAppApplicationTabsBook" title="AppBook">
                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                    <netuix:content>
                      <!-- Definition for the Overview tab -->
                      <netuix:page markupName="page" definitionLabel="WebAppApplicationOverviewPage"
                                   presentationClass="page-content"
                                   title="tab.overview.label">
                        <netuix:meta name="helpid" content="J2EE.webapp.webappoverview.title;j2ee"/>
                        <netuix:meta name="breadcrumb-context" content="handle"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="WebAppApplicationOverviewPortlet">
                                <netuix:strutsContent module="/j2ee" action="WebAppApplicationOverview"
                                                      refreshAction="WebAppApplicationOverview"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the Deployment Plan tab -->
                      <netuix:book markupName="book" definitionLabel="WebAppApplicationDeploymentPlanPage"
                                   title="tab.deploymentplan.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Tuning Parameters tab -->
                          <netuix:page markupName="page" definitionLabel="WebAppApplicationTuningParameters"
                                       presentationClass="page-content"
                                       title="tab.tunables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationtunables.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Tuning Parameter pages -->
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="AppApplicationTunablesHeaderPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationTunablesHeader.portlet"/>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="WebAppApplicationTuningParametersBook">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="WebAppApplicationTunablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupName="twoColumnLayout" markupType="Layout">
                                            <!-- Navigation tree shared by all Tuning Parameter pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppApplicationTunablesTreePortlet"
                                                                      contentUri="/PortalConfig/app/ApplicationTunablesTree.portlet"/>
                                            </netuix:placeholder>
                                            <!-- Content pane for Tuning Parameter pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="WebAppApplicationTunablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab configuration page -->
                                                  <netuix:book markupName="book"
                                                               definitionLabel="WebAppApplicationTunablesContentsDefaultPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page"
                                                                   definitionLabel="WebAppApplicationTunablesContentsDefaultPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="WebAppApplicationTunablesContentsDefaultPortlet"
                                                                                      contentUri="/PortalConfig/app/ApplicationTunablesContentsDefault.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The two tunable tabs for a WebApp module -->
                                                  <netuix:book markupName="book" definitionLabel="WebAppApplicationTunablesWebAppPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="WebAppApplicationTunablesGeneralPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.general.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppApplicationTunablesGeneralPortlet"
                                                                                      contentUri="/PortalConfig/webapp/WebAppTunablesGeneral.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                      <netuix:page markupName="page" definitionLabel="WebAppApplicationTunablesLoggingPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.logging.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppApplicationTunablesLoggingPortlet"
                                                                                      contentUri="/PortalConfig/webapp/WebAppTunablesLogging.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The single tunable tab for a web service -->
                                                  <netuix:book markupName="book" definitionLabel="WebAppApplicationWebServiceTunablesPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page"
                                                                   definitionLabel="WebAppApplicationWebServiceTunablesGeneralPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.general.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="WebAppApplicationWebServiceTunablesGeneralPortlet"
                                                                                      contentUri="/PortalConfig/webservice/WebServiceTunablesGeneral.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book"
                                                               definitionLabel="WebAppApplicationTunablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="WebAppApplicationTunablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="WebAppApplicationTunablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Resource Dependencies tab -->
                          <netuix:page markupName="page" definitionLabel="WebAppApplicationBindablesPages"
                                       presentationClass="page-content"
                                       title="tab.bindables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationdescriptors.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Dependencies pages -->
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppApplicationBindablesPortlet"
                                                          contentUri="/PortalConfig/app/ApplicationBindablesHeader.portlet"/>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="WebAppApplicationBindablesContentsBooks">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="WebAppApplicationBindablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Dependencies pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppApplicationBindablesTreePortlet"
                                                                      contentUri="/PortalConfig/app/ApplicationBindablesTree.portlet"/>
                                            </netuix:placeholder>
                                            <!-- Content pane for Dependencies pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="WebAppApplicationBindablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab resource configuration page -->
                                                  <netuix:book markupName="book" definitionLabel="WebAppApplicationBindablesContentsPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="WebAppApplicationBindablesContentsPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="WebAppApplicationBindablesContentsPortlet"
                                                                                      contentUri="/PortalConfig/app/ApplicationBindablesContents.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book"
                                                               definitionLabel="WebAppApplicationBindablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page"
                                                                   definitionLabel="WebAppApplicationBindablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="WebAppApplicationBindablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the Configuration tab -->
                      <netuix:book markupName="book" definitionLabel="WebAppApplicationConfigurationPage"
                                   title="tab.configuration.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- definition for General subtab -->
                          <netuix:page markupName="page" definitionLabel="WebAppApplicationConfigurationGeneralPage"
                                       presentationClass="page-content"
                                       title="tab.general.label">
                            <netuix:meta name="helpid" content="J2EE.webapp.webappconfiguration.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="WebAppApplicationConfigurationGeneralPortlet"
                                                          contentUri="/PortalConfig/webapp/WebAppConfiguration.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for Logging subtab -->
                          <netuix:page markupName="page" definitionLabel="WebAppApplicationConfigurationLoggingPage"
                                       presentationClass="page-content"
                                       title="tab.logging.label">
                            <netuix:meta name="helpid" content="J2EE.webapp.webappconfigurationlogging.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="WebAppApplicationConfigurationLoggingPortlet"
                                                          contentUri="/PortalConfig/webapp/WebAppConfigurationLogging.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Persistence tab -->
                          <netuix:page markupName="page" definitionLabel="WebAppApplicationPersistenceUnitTablePage"
                                       presentationClass="page-content"
                                       title="tab.persistenceunit.table.label">
                            <netuix:meta name="helpid" content="J2EE.persistenceunit.table.title;j2ee"/>
                            <netuix:meta name="breadcrumb-context" content="handle"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="WebAppApplicationPersistenceUnitTablePortlet"
                                                          contentUri="/PortalConfig/persistence/PersistenceUnitTable.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Workload tab -->
                          <netuix:page markupName="page" definitionLabel="WebAppApplicationConfigurationWorkloadPage"
                                       presentationClass="page-content"
                                       title="tab.workload.label">
                            <netuix:meta name="helpid" content="J2EE.webapp.webappconfigurationworkload.title;j2ee"/>
                            <netuix:meta name="breadcrumb-context" content="handle"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="WebAppApplicationConfigurationWorkloadPortlet">
                                    <netuix:strutsContent module="/j2ee" action="WebAppApplicationConfigurationWorkload"
                                                          refreshAction="WebAppApplicationConfigurationWorkload"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- definition for the Instrumentation subtab-->
                          <netuix:page markupName="page"
                                       definitionLabel="WebAppApplicationConfigurationInstrumentationPage"
                                       presentationClass="page-content"
                                       title="tab.diagnostics.instrumentation.label">
                            <netuix:meta name="helpid"
                                         content="Diagnostics.diagnostics.moduleconfiginstrumentation.title;diagnostics"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="WebAppApplicationConfigurationInstrumentationPortlet"
                                                          contentUri="/PortalConfig/diagnostics/module/ModuleConfigInstrumentation.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the security tab -->
                      <netuix:book markupName="book" definitionLabel="WebAppApplicationSecurityPage"
                                   title="tab.security.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <netuix:book markupName="book" definitionLabel="WebAppApplicationApplicationSecurityPage"
                                       title="tab.applicationscope.label">
                            <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level2"/>
                            <netuix:content>
                              <!-- definition for standalone web application roles table page -->
                              <netuix:page markupName="page" definitionLabel="WebAppApplicationSecurityRolesTablePage"
                                           presentationClass="page-content"
                                           title="tab.roles.label">
                                <netuix:meta name="helpid" content="J2EE.webapp.application.role.table.page.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.roles.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="WebAppApplicationSecurityRolesTablePortlet">
                                        <netuix:strutsContent module="/j2ee" action="WebAppApplicationRolesTable"
                                                              refreshAction="WebAppApplicationRolesTable"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                              <!-- definition for standalone web application policies page -->
                              <netuix:page markupName="page" definitionLabel="WebAppApplicationSecurityPoliciesPage"
                                           presentationClass="page-content"
                                           title="tab.policies.label">
                                <netuix:meta name="helpid" content="J2EE.webapp.application.policies.page.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.policies.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="WebAppApplicationSecurityPoliciesPortlet">
                                        <netuix:pageflowContent contentUri="/com/bea/console/actions/webapp/policies/edit/app/EditWebAppApplicationPolicies.jpf"
                                                                action="begin"
                                                                refreshAction="begin"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                            </netuix:content>
                          </netuix:book>
                          <netuix:book markupName="book" definitionLabel="WebAppApplicationURLPatternSecurityPage"
                                       title="tab.urlpatternscope.label">
                            <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level2"/>
                            <netuix:content>
                              <!-- definition for WebAppApplicationRolesURLPatternTable page -->
                              <netuix:page markupName="page" definitionLabel="WebAppApplicationRolesURLPatternTablePage"
                                           presentationClass="page-content"
                                           title="tab.roles.label">
                                <netuix:meta name="helpid"
                                             content="J2EE.webapp.application.urlpatterns.role.table.page.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.roles.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="WebAppApplicationRolesURLPatternTablePortlet">
                                        <netuix:strutsContent module="/j2ee" action="WebAppApplicationRolesURLPatternTable"
                                                              refreshAction="WebAppApplicationRolesURLPatternTable"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                              <!-- definition for WebAppApplicationPoliciesURLPatternTable page -->
                              <netuix:page markupName="page"
                                           definitionLabel="WebAppApplicationPoliciesURLPatternTablePage"
                                           presentationClass="page-content"
                                           title="tab.policies.label">
                                <netuix:meta name="helpid"
                                             content="J2EE.webapp.application.urlpatterns.policy.table.page.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.policies.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="WebAppApplicationPoliciesURLPatternTablePortlet">
                                        <netuix:strutsContent module="/j2ee" action="WebAppApplicationPoliciesURLPatternTable"
                                                              refreshAction="WebAppApplicationPoliciesURLPatternTable"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                            </netuix:content>
                          </netuix:book>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the targets tab -->
                      <netuix:page markupName="page" definitionLabel="WebAppApplicationTargetsPage"
                                   presentationClass="page-content"
                                   title="tab.targets.label">
                        <netuix:meta name="helpid" content="J2EE.webapp.webapptargets.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="WebAppApplicationTargetsPortlet">
                                <netuix:strutsContent module="/j2ee" action="WebAppTargets" refreshAction="WebAppTargets"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the control tab -->
                      <netuix:page markupName="page" definitionLabel="WebAppApplicationControlPage"
                                   presentationClass="page-content"
                                   title="tab.control.label">
                        <netuix:meta name="helpid" content="J2EE.webapp.webappcontrol.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="WebAppApplicationControlPortlet">
                                <netuix:strutsContent module="/j2ee" action="WebAppControl" refreshAction="WebAppControl"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
